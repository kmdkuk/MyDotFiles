[alias]
	s = stash
	sp = stash pop
	st = status
	sh = show
	sb = show-branch
	cm = commit
	co = checkout
	cob = checkout -b
	dc = diff --cached
	df = diff
	wdf = diff --word-diff
	a = add
	aall = add -A
	uadd = restore --staged
	tree = log --graph --all --format=\"%x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(magenta reverse)%d%Creset %s\"
	delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
