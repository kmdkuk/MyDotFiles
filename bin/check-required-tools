#!/usr/bin/env bash

function check_command() {
    if which $1 > /dev/null 2>&1; then
        return
    fi
    echo "required $1, please install"
}

function check_file() {
    if [ -f $1 ]; then
        return
    fi
    echo "not found $1"
}

check_command ghq
check_command peco
check_command starship
